<h1>{{user.firstName}} {{user.lastName}}</h1>

<div class="form-group">
  <label for="staticUsername" class="col-sm-2 col-form-label">Username</label>
    <div class="col-sm-10">
      <input class="form-control" type="text" placeholder={{user.username}} readonly>
    </div>
</div>

<div class="form-group">
  <label for="staticEmail" class="col-sm-2 col-form-label">Email</label>
    <div class="col-sm-10">
      <input class="form-control" type="text" placeholder={{user.email}} readonly>
    </div>
</div>

<div class="form-group">
  <label for="staticPhoneNumber" class="col-sm-2 col-form-label">Phone Number</label>
    <div class="col-sm-10">
      <input class="form-control" type="text" placeholder={{user.phoneNumber}} readonly>
    </div>
</div>

